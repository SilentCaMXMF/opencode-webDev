{
  "name": "frontend-design-monitoring",
  "version": "1.0.0",
  "description": "Performance monitoring dashboard for Frontend Design Agent System",
  "main": "index.js",
  "scripts": {
    "collector:start": "ts-node collector/src/index.ts",
    "collector:dev": "ts-node-dev collector/src/index.ts",
    "dashboard:dev": "cd dashboard && npm run dev",
    "dashboard:build": "cd dashboard && npm run build",
    "dashboard:start": "cd dashboard && npm start",
    "db:migrate": "ts-node database/scripts/migrate.ts",
    "db:seed": "ts-node database/scripts/seed.ts",
    "db:reset": "ts-node database/scripts/reset.ts",
    "alerting:start": "ts-node alerting/src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/node": "^20.10.0",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "prom-client": "^15.1.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "zod": "^3.22.4",
    "nodemailer": "^6.9.7",
    "axios": "^1.6.2",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/pg": "^8.10.9",
    "@types/nodemailer": "^6.4.14",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
